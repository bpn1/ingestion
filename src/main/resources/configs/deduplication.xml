<?xml version="1.0" encoding="UTF-8"?>
<config>
    <settings>
        <keyspaceSubjectTable>datalake</keyspaceSubjectTable>
        <subjectTable>subject</subjectTable>
        <keyspaceStagingTable>datalake</keyspaceStagingTable>
        <stagingTable>subject_temp</stagingTable>
        <keyspaceDuplicatesTable>datalake</keyspaceDuplicatesTable>
        <duplicatesTable>duplicate_candidates</duplicatesTable>
        <keyspaceVersionTable>datalake</keyspaceVersionTable>
        <versionTable>version</versionTable>
        <filterUndefined>true</filterUndefined>
        <maxBlockSize>500000</maxBlockSize>
        <confidence>0.7</confidence>
    </settings>
    <simMeasurements>
        <attribute>
            <key>name</key>
            <weight>0.6</weight>
            <feature>
                <similarityMeasure>JaroWinkler</similarityMeasure>
                <weight>1</weight>
                <scale>1</scale>
            </feature>
        </attribute>
        <attribute>
            <key>gen_employees</key>
            <weight>0.2</weight>
            <feature>
                <similarityMeasure>RoughlyEqualNumbers</similarityMeasure>
                <weight>1.0</weight>
                <scale>1</scale>
            </feature>
        </attribute>
        <attribute>
            <key>geo_city</key>
            <weight>0.2</weight>
            <feature>
                <similarityMeasure>Jaccard</similarityMeasure>
                <weight>1.0</weight>
                <scale>1</scale>
            </feature>
        </attribute>
    </simMeasurements>
</config>
