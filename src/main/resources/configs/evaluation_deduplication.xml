<?xml version="1.0" encoding="UTF-8"?>
<config>
    <settings>
        <keyspaceSubjectTable>evaluation</keyspaceSubjectTable>
        <subjectTable>subject_dbpedia</subjectTable>
        <keyspaceStagingTable>evaluation</keyspaceStagingTable>
        <stagingTable>subject_wikidata</stagingTable>
        <keyspaceGoldStandardTable>evaluation</keyspaceGoldStandardTable>
        <goldStandardTable>goldstandard</goldStandardTable>
        <keyspaceDuplicatesTable>evaluation</keyspaceDuplicatesTable>
        <duplicatesTable>dbpedia_wikidata_deduplication_only_uuid</duplicatesTable>
        <keyspaceVersionTable>evaluation</keyspaceVersionTable>
        <versionTable>version</versionTable>
        <keyspaceStatsTable>datalake</keyspaceStatsTable>
        <statsTable>blockingstats</statsTable>
        <filterUndefined>false</filterUndefined>
        <filterSmall>true</filterSmall>
        <maxBlockSize>500000</maxBlockSize>
        <confidence>0.7</confidence>
    </settings>
    <simMeasurements>
        <attribute>
            <key>name</key>
            <feature>
                <similarityMeasure>JaroWinkler</similarityMeasure>
                <weight>1</weight>
                <scale>1</scale>
            </feature>
        </attribute>
        <attribute>
            <key>geo_country</key>
            <feature>
                <similarityMeasure>ExactMatch</similarityMeasure>
                <weight>0.5</weight>
                <scale>1</scale>
            </feature>
            <feature>
                <similarityMeasure>Jaccard</similarityMeasure>
                <weight>1</weight>
                <scale>1</scale>
            </feature>
        </attribute>
    </simMeasurements>
</config>
